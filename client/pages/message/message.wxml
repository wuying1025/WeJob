<view class="contain">
  
  <view wx:for="{{msgList}}" class="job">
  <navigator url='../user/position/position' hover-class='none'>
  <image src="{{item.logo}}" class="job-img"></image>
  <view class='job-info'>
   <view>{{item.name}}</view>
    {{item.city}}
    {{item.posttime}}发布
    <view class="salary-color">￥{{item.salary}}/天</view>
    <button class='job-apply' hover-class='none'>申请</button>
  </view>
  </navigator></view>
</view>
